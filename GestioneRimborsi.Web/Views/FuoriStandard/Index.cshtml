<script>

    $(".application-title").text("Gestione Fuori Standard");

    $(function () {
        $('#dataFine').datetimepicker();
    });

    $(function () {
        $('#dataInizio').datetimepicker();
    });

    $("#Tutti").css({
        "background-color": "#3E7819",
