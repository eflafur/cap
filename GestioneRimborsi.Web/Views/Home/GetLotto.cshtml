@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="Container">

    <div class="row">
        <button type="button" class="btn btn-success" id="btn1">Success</button>
        
        <div class=" col-md-12" id="tbf1">
            <table id="tablef1" class="table table-bordered" ">
                <thead id="thd">
                    <tr>
                        <th>Valida</th>
                        <th>Settore</th>
                        <th>User</th>
                        <th>Leader</th>
                        <th>Impiegati</th>
                    </tr>
                </thead>
                <tbody id="tb61">
                    @foreach (var item in ViewBag.settore)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.Desc</td>
                            <td>@item.CreateByName</td>
                            <td>@item.DateCreation</td>
                            <td>@item.ModifiedBy</td>
                            <td>@item.DateModified</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

    <div class="row">
      


        <div class="input-group">
            <p>Utente<input type="text" id="nome" /></p>
            <p>Capitale<input type="number" id="fondi" /></p>
        </div>

        <p>"totale settore: " @ViewBag.settore[1].Citta</p>
    </div>

</div>



